<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_test_home_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="@drawable/soccer"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.michael.fantasyheadtoheadgame.TestHomeScreen"
    tools:showIn="@layout/activity_test_home_screen">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginStart="15dp"
        android:onClick="changePlayer"
        android:layout_marginTop="26dp"
        android:id="@+id/xmlchangePlayer2Bt"
        android:layout_below="@+id/xmlchangePlayer1Bt"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:backgroundTint="@android:color/transparent"
        android:layout_below="@+id/xmlchangePlayer3Bt"
        android:onClick="changePlayer"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:id="@+id/xmlchangePlayer5Bt" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changePlayer"
        app:srcCompat="@mipmap/ic_launcher"
        android:backgroundTint="@android:color/transparent"
        android:layout_below="@+id/xmlchangePlayer8Bt"
        android:layout_alignEnd="@+id/xmlchangePlayer6Bt"
        android:id="@+id/xmlchangePlayer9Bt" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@mipmap/ic_launcher"
        android:onClick="changePlayer"
        android:layout_below="@+id/xmlchangePlayer8Bt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:id="@+id/xmlchangePlayer10Bt" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:onClick="changePlayer"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/xmlchangePlayer8Bt"
        android:layout_below="@+id/xmlchangePlayer7Bt"
        android:layout_alignStart="@+id/xmlchangePlayer5Bt" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer2Bt"
        android:layout_alignEnd="@+id/xmlchangePlayer2Bt"
        android:id="@+id/xmlPlayer2Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/xmlPlayer2Name"
        android:layout_alignEnd="@+id/xmlPlayer1Name"
        android:id="@+id/xmlPlayer3Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/xmlPlayer3Name"
        android:layout_alignEnd="@+id/xmlchangePlayer4Bt"
        android:id="@+id/xmlPlayer4Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer6Bt"
        android:layout_alignStart="@+id/xmlchangePlayer6Bt"
        android:id="@+id/xmlPlayer6Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer9Bt"
        android:layout_alignEnd="@+id/xmlchangePlayer9Bt"
        android:id="@+id/xmlPlayer9Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer8Bt"
        android:layout_alignStart="@+id/xmlPlayer10Name"
        android:id="@+id/xmlPlayer8Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer7Bt"
        android:layout_alignEnd="@+id/xmlchangePlayer7Bt"
        android:id="@+id/xmlPlayer7Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer11Bt"
        android:layout_alignEnd="@+id/xmlchangePlayer11Bt"
        android:id="@+id/xmlPlayer11Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xmlchangePlayer5Bt"
        android:layout_alignEnd="@+id/xmlPlayer3Name"
        android:id="@+id/xmlPlayer5Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer10Name"
        android:layout_below="@+id/xmlchangePlayer10Bt"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:layout_height="wrap_content"
        android:onClick="changePlayer"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginEnd="27dp"
        android:id="@+id/xmlchangePlayer4Bt"
        android:layout_above="@+id/xmlPlayer4Name"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:onClick="changePlayer"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/xmlchangePlayer7Bt"
        android:layout_alignTop="@+id/xmlchangePlayer6Bt"
        android:layout_alignEnd="@+id/xmlPlayer4Name" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@mipmap/ic_launcher"
        android:onClick="changePlayer"
        android:id="@+id/xmlchangePlayer11Bt"
        android:layout_below="@+id/xmlchangePlayer8Bt"
        android:layout_alignEnd="@+id/xmlPlayer7Name" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changePlayer"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/xmlchangePlayer6Bt"
        android:layout_centerVertical="true"
        android:layout_alignEnd="@+id/xmlPlayer2Name" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changePlayer"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/xmlchangePlayer3Bt"
        android:layout_above="@+id/xmlPlayer3Name"
        android:layout_alignStart="@+id/xmlchangePlayer5Bt" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"

        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:onClick="openSearchScreen"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_search_category_default"
        android:background="#778cc29d"
        android:id="@+id/imageButton2"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/xmlPlayer2Name"
        android:layout_marginStart="18dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:onClick="findHeadToHead"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/star_big_on"
        android:id="@+id/imageButton3"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_alignStart="@+id/xmlPlayer4Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer1Name"
        android:layout_marginBottom="9dp"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="60dp"
        android:backgroundTint="@android:color/transparent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:onClick="changePlayer"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/green_jersey_hi"
        android:id="@+id/xmlchangePlayer1Bt"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
