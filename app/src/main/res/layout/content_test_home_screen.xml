<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_test_home_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:background="@drawable/soccer"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.michael.fantasyheadtoheadgame.ActivityScreens.UserTeamScreen"
    tools:showIn="@layout/activity_test_home_screen">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/tottenham"
        android:onClick="changePlayer"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/imageButton9"
        android:layout_below="@+id/imageButton7"
        android:layout_alignStart="@+id/imageButton5"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:onClick="changePlayer"
        android:backgroundTint="@android:color/transparent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/tottenham"
        android:id="@+id/imageButton13"
        android:layout_below="@+id/imageButton11"
        android:layout_toStartOf="@+id/imageButton10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changePlayer"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/tottenham"
        android:id="@+id/imageButton12"
        android:layout_below="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton3" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        android:textColor="#000000"
        android:layout_below="@+id/imageButton11"
        android:layout_centerHorizontal="true"
        android:id="@+id/xmlPlayer9Name" />

    <TextView
        android:text="TextView"
        android:layout_width="40dp"
        android:textColor="#000000"
        
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_below="@+id/imageButton13"
        android:layout_alignStart="@+id/imageButton13"
        android:layout_alignEnd="@+id/imageButton13"
        android:id="@+id/xmlPlayer11Name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        
        android:id="@+id/xmlPlayer5Name"
        android:layout_below="@+id/imageButton7"
       android:layout_centerHorizontal="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        
        android:id="@+id/xmlPlayer7Name"
        android:layout_below="@+id/imageButton9"
        android:layout_centerHorizontal="true"  />

    <TextView
        android:text="TextView"
        android:layout_width="40dp"
        
        android:textColor="#000000"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer10Name"
        android:layout_below="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton12"
        android:layout_alignEnd="@+id/imageButton12" />

    <ImageButton
        android:layout_width="60dp"
        android:backgroundTint="@android:color/transparent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:onClick="changePlayer"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/goalie"
        android:id="@+id/xmlchangePlayer1Bt"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:id="@+id/xmlPlayer1Name"
        
        android:layout_below="@+id/xmlchangePlayer1Bt"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="TextView"
        android:layout_width="40dp"
        android:textColor="#000000"
        android:gravity="center"
        
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer2Name"
        android:layout_below="@+id/imageButton4"
        android:layout_alignStart="@+id/imageButton4"
        android:layout_alignEnd="@+id/imageButton4" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/tottenham"
        android:onClick="changePlayer"
        android:id="@+id/imageButton5"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignEnd="@+id/xmlchangePlayer1Bt" />


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/tottenham"
        android:onClick="changePlayer"
        android:id="@+id/imageButton4"
        android:layout_marginTop="7dp"
        android:paddingLeft="10dp"
        android:layout_marginStart="11dp"
        android:layout_below="@+id/xmlPlayer1Name"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer3Name"
        android:layout_below="@id/imageButton5" 
        android:layout_centerHorizontal="true"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:onClick="changePlayer"
        app:srcCompat="@drawable/tottenham"
        android:id="@+id/imageButton10"
        android:layout_alignTop="@+id/xmlPlayer5Name"
        android:layout_alignStart="@+id/xmlPlayer4Name"
        android:layout_above="@+id/xmlPlayer6Name"
        android:layout_alignEnd="@+id/xmlPlayer4Name" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/tottenham"
        android:onClick="changePlayer"
        android:id="@+id/imageButton8"
        android:layout_alignTop="@+id/xmlPlayer5Name"
        android:layout_alignStart="@+id/xmlPlayer2Name" />

    <TextView
        android:text="TextView"
        android:layout_width="40dp"
        android:textColor="#000000"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer6Name"
        android:layout_below="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8"
        android:layout_alignEnd="@+id/imageButton8" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/tottenham"
        android:onClick="changePlayer"
        android:id="@+id/imageButton11"
        android:layout_below="@+id/xmlPlayer8Name"
        android:layout_alignEnd="@+id/xmlPlayer9Name"
        android:layout_alignStart="@+id/imageButton9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:onClick="openSearchScreen"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_search_category_default"
        android:background="#778cc29d"
        android:id="@+id/imageButton2"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:onClick="findHeadToHead"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/h2h_opt"
        android:id="@+id/imageButton3"
        android:layout_marginBottom="9dp"
        android:layout_alignBottom="@+id/xmlchangePlayer1Bt"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:id="@+id/xmlPlayer4Name"
        android:layout_alignBaseline="@+id/xmlPlayer3Name"
        android:layout_alignBottom="@+id/xmlPlayer3Name"
        android:layout_alignStart="@+id/imageButton6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:onClick="changePlayer"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/tottenham"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/imageButton7"
        android:layout_below="@+id/xmlPlayer3Name"
        android:layout_alignStart="@+id/imageButton9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/tottenham"
        android:onClick="changePlayer"
        android:layout_marginEnd="30dp"
        android:id="@+id/imageButton6"
        android:layout_above="@+id/xmlPlayer4Name"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:id="@+id/xmlPlayer8Name"
        android:layout_below="@+id/imageButton10"
        android:layout_alignStart="@+id/imageButton10" />
</RelativeLayout>
